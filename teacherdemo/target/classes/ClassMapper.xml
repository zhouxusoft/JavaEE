<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="classMapper">

    <select id="selectById" parameterType="int" resultType="top.godxu.pojo.Class">
        select * from classes where id = #{id}
    </select>

    <select id="selectAll" resultType="top.godxu.pojo.Class">
        select * from classes
    </select>

</mapper>